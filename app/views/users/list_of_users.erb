<form action="/submit_logout" method="post">
  <input type="submit" value="Log Out!">
</form>

<h1>List of all users</h1>

<ul>
  <% @users.each do |user| %>
  <li>
    <%= user.id %> - 
    <%= user.last_name %>, 
    <%= user.first_name %> - 
    <a href="#">add friend</a> - 
  </li>
  <% end %>
</ul>

